  },
  "devDependencies": {
    "chai": "^4.0.1",
    "codecov": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-warp": "^3.0.0",
    "esm": "^3.0.84",
    "fluture": "^11.0.0",
